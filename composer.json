{
    "name": "codeages/plumber",
    "type": "library",
    "description": "A PHP Message Queue Worker.",
    "keywords": ["plumber", "beanstalk", "message queue", "swoole"],
    "homepage": "https://github.com/codeages/plumber",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codeages\\Plumber\\": "src/",
            "Codeages\\Plumber\\Tests\\": "tests/",
            "Codeages\\Plumber\\Example\\": "example/"
        }
    },
    "authors": [
        {
            "name": "Codeages Team",
            "email": "team@codeages.com"
        }
    ],
    "require": {
        "php": ">=5.4.1",
        "ext-swoole": ">=1.7.17",
        "pimple/pimple": "^3",
        "docopt/docopt": "^1",
        "codeages/beanstalk-client" : "^0.1",
        "codeages/rate-limiter": "^0.2",
        "monolog/monolog": "^1.22"
    },
    "bin": ["bin/plumber"],
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^4|^5|6"
    }
}
